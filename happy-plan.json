{
	"name": "MoOx.fr",
	"url": "http://moox.fr/",
	"description": "Web Developer Front-End",
	"baseUrl": "/",
	"paths": {
		"src": {
			"_": "",
			"scripts": "<%= config.paths.src._ %>scripts/",
			"styles": "<%= config.paths.src._ %>styles/",
			"images": "<%= config.paths.src._ %>images/",
			"fonts": "<%= config.paths.src._ %>fonts/",
			"webfonts": "<%= config.paths.src._ %>webfonts/",
			"components": "<%= config.paths.src._ %>components/",
			"medias": "<%= config.paths.src._ %>medias/",
		},
		"build": {
			"_": "dist/",
			"scripts": "<%= config.paths.build._ %>_/js/",
			"styles": "<%= config.paths.build._ %>_/css/",
			"images": "<%= config.paths.build._ %>_/i/",
			"fonts": "<%= config.paths.build._ %>_/f/",
			"webfonts": "<%= config.paths.build._ %>_/f/",
			"components": "<%= config.paths.build._ %>_/c/",
			"medias": "<%= config.paths.build._ %>medias/"
		}
	},
	"src": {
		"_path": "src",
		"assets": {
			"_path": "src/_assets",
			"scripts": "src/_assets/scripts",
			"styles": "src/_assets/styles",
			"images": "src/_assets/images",
			"fonts": "src/_assets/fonts",
			"webfonts": "src/_assets/webfonts",
			"components": "src/_assets/components"
		},
		"medias": "medias"
	},
	"build": {
		"_path": "build",
		"assets": {
			"_path": "build/assets",
			"scripts": "build/assets/js",
			"styles": "build/assets/css",
			"images": "build/assets/i",
			"fonts": "build/assets/fonts",
			"webfonts": "build/assets/fonts",
			"components": "build/assets/components"
		},
		"medias": "medias"
	},
	"assets": {
		"main": {
			"style": "style.css",
			"script": "vanilla.css"
		}
	},
	"jshint": {
		"files": [
			"Gruntfile.js",
			"component.json",
			"happy-plan.json",
			"package.json",
			"<%= happyPlan.src.assets.scripts %>/**/*.js",
			"!<%= happyPlan.src.assets.scripts %>/lib/**/*",
			".ftppass"
		]
	},
	"jekyll": {
		"permalink": "/:categories/:title"
	},
	"compass": {
		"require": [
			"modular-scale",
			"ceaser-easing",
			"compass-recipes"
		],
		"additional_import_paths": [
			"<%= happyPlan.src.assets.components %>/",
			"<%= happyPlan.src.assets.components %>/griddle/",
			"<%= happyPlan.src.assets.components %>/hint.css/src/",
			"<%= happyPlan.src.assets.components %>/pygments-sass-base-theme/",
			"<%= happyPlan.src.assets.components %>/"
		],
		"meh": "last path is stupid, it's because of a compass import path issue I think. I should take a deeper look."
	},
	"uglify": {
		"options": {
			"banner": "/*! <%= pkg.name %> - v<%= pkg.version %> - <%= grunt.template.today('yyyy-mm-dd') %> */\n"
		},
		"files": {
			"<%= happyPlan.build.assets.scripts %>/ieshim.js": ["<%= happyPlan.src.assets.scripts %>/lib/aight.js"],
			"<%= happyPlan.build.assets.scripts %>/vanilla.js": [
				"<%= happyPlan.src.assets.components %>/picturefill/external/matchmedia.js",
				"<%= happyPlan.src.assets.components %>/picturefill/picturefill.js",
				"<%= happyPlan.src.assets.scripts %>/vanilla.js"
			]
		}
	},
	"components": {
		"files": []
	}
}
