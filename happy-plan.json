{
	"name": "MoOx.fr",
	"url": "http://moox.fr/",
	"description": "Web Developer Front-End",
	"baseUrl": "",
	"paths": {
		"src": {
			"scripts": "scripts/",
			"styles": "styles/",
			"images": "images/"
		},
		"build": {
			"scripts": "build/assets/js/",
			"styles": "build/assets/css/",
			"images": "build/img/"
		}
	},
	"src": {
		"_path": "src",
		"assets": {
			"_path": "src/_assets",
			"scripts": "src/_assets/scripts",
			"styles": "src/_assets/styles",
			"images": "src/_assets/images",
			"fonts": "src/_assets/fonts",
			"webfonts": "src/_assets/webfonts",
			"components": "src/_assets/components"
		},
		"medias": "medias"
	},
	"build": {
		"_path": "build",
		"assets": {
			"_path": "build/assets",
			"scripts": "build/assets/js",
			"styles": "build/assets/css",
			"images": "build/assets/i",
			"fonts": "build/assets/fonts",
			"webfonts": "build/assets/fonts",
			"components": "build/assets/components"
		},
		"medias": "medias"
	},
	"jshint": {
		"files": [
			"Gruntfile.js",
			"component.json",
			"happy-plan.json",
			"package.json",
			"<%= happyPlan.src.assets.scripts %>/**/*.js",
			"!<%= happyPlan.src.assets.scripts %>/lib/**/*",
			".ftppass"
		]
	},
	"jekyll": {
		"permalink": "/:categories/:title"
	},
	"compass": {
		"require": [
			"modular-scale",
			"ceaser-easing",
			"compass-recipes"
		],
		"add_import_path": [
			"<%= happyPlan.src.assets.components %>"
		]
	},
	"uglify": {
		"options": {
			"banner": "/*! <%= pkg.name %> - v<%= pkg.version %> - <%= grunt.template.today('yyyy-mm-dd') %> */\n"
		},
		"files": {
			"<%= happyPlan.build.assets.scripts %>/lib/aight.js": ["<%= happyPlan.src.assets.scripts %>/lib/aight.js"],
			"<%= happyPlan.build.assets.scripts %>/vanilla.js": ["<%= happyPlan.src.assets.scripts %>/vanilla.js"]
		}
	},
	"components": {
		"files": []
	}
}
