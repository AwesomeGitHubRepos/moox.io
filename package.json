{
  "private": true,
  "name": "moox.io",
  "version": "0.0.0",
  "devDependencies": {
    "@glennsl/bs-json": "^3.0.0",
    "@moox/bs-react-helmet": "^2.0.0",
    "@phenomic/cli": "^1.0.0-beta.11",
    "@phenomic/core": "^1.0.0-beta.11",
    "@phenomic/preset-react-app": "^1.0.0-beta.11",
    "@svgr/cli": "^3.1.0",
    "babel-jest": "^21.2.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-react-native-web": "^0.3.3",
    "babel-preset-react-native": "^4.0.0",
    "bs-date-fns": "^0.1.6",
    "bs-fetch": "^0.3.0",
    "bs-json": "^1.0.1",
    "bs-platform": "^5.0.6",
    "bs-react-native-jsx3-compat": "https://github.com/reason-react-native/bs-react-native-jsx3-compat.git",
    "bs-webapi": "^0.15.1",
    "husky": "^1.1.4",
    "jest": "^21.2.1",
    "lint-staged": "^8.0.5",
    "matomo-tracker": "^2.1.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.15.0",
    "react": "^16.8.1",
    "react-art": "^16.8.1",
    "react-disqus-comments": "^1.2.0",
    "react-dom": "^16.8.1",
    "react-helmet": "^6.0.0-beta",
    "react-multiversal": "https://github.com/MoOx/react-multiversal.git#e4ee89e",
    "react-native-web": "^0.9.8",
    "react-router": "^3.2.0",
    "react-test-renderer": "^16.8.1",
    "reason-react": "https://github.com/reasonml/reason-react.git",
    "reason-react-native": "https://github.com/reason-react-native/reason-react-native.git#9012a7b",
    "trash-cli": "^3.1.0"
  },
  "scripts": {
    "format": "yarn format:most && yarn format:re",
    "format:most": "prettier --write \"**/*.{js,json,css,md}\" \"!package.json\"",
    "format:re": "find . -name \"*.re\" | grep -v \"node_modules\" | xargs bsrefmt --in-place",
    "clean": "yarn re:clean && yarn svg:clean",
    "re:clean": "bsb -clean-world",
    "re:build": "bsb -make-world",
    "svg:convert:web": "svgr --icon -d assets/SVGs/components assets/SVGs",
    "postsvg:convert:web": "cd assets/SVGs/components && find ./ -name \"*.js\" | xargs -I '{}' basename '{}' | sed 's/\\.js//' | xargs -I '{}' mv '{}.js'  '{}.web.js'",
    "svg:clean": "trash assets/SVGs/components",
    "svg:convert:native": "svgr --icon --native -d assets/SVGs/components assets/SVGs",
    "svg:convert:re": "node scripts/generate-svg-reason.js assets/SVGs/components",
    "svg:convert": "trash assets/SVGs/components && yarn svg:convert:web && yarn svg:convert:native && yarn svg:convert:re",
    "prepare": "yarn clean && yarn svg:convert && yarn re:build",
    "start": "echo Choose start:web or start:native",
    "start:re": "yarn re:build -w",
    "start:web": "npm-run-all --parallel start:web:*",
    "start:web:js": "phenomic start",
    "start:web:re": "yarn start:re",
    "build:web": "yarn re:build && phenomic build",
    "test": "jest"
  },
  "prettier": {
    "trailingComma": "all",
    "proseWrap": "always"
  },
  "lint-staged": {
    "*.re": [
      "bsrefmt --in-place",
      "git add"
    ],
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "phenomic": {
    "presets": [
      "@phenomic/preset-react-app"
    ]
  },
  "jest": {
    "preset": "react-native"
  }
}
